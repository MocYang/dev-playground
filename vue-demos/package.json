{
  "name": "fronbase",
  "version": "1.0.0",
  "description": "A Vue.js project",
  "author": "MocYang <moclinging@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --mode development --inline --progress --config=build/webpack.dev.conf.js",
    "watch": "cross-env NODE_ENV=watch webpack --mode development --watch --progress --hide-modules --config=build/webpack.prod.conf.js",
    "build": "node build/build.js",
    "standard": "standard --fix --verbose",
    "lint": "eslint --ext .js,.vue src",
    "precommit": "lint-staged"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "keywords": [],
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "standard": "^11.0.1",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^8.5.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.5",
    "cross-spawn": "^6.0.5",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-vue": "^4.5.0",
    "faker": "^4.1.0",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "imagemin-lint-staged": "^0.3.0",
    "lint-staged": "^7.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "mockjs": "^1.0.1-beta3",
    "moment": "^2.22.2",
    "node-notifier": "^5.2.1",
    "node-sass": "^4.8.3",
    "optimize-css-assets-webpack-plugin": "^4.0.1",
    "portfinder": "^1.0.13",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "postcss-url": "^7.3.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "semver": "^5.5.0",
    "shelljs": "^0.8.2",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "vue-awesome-swiper": "^3.1.3",
    "vue-loader": "^14",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.8.3",
    "webpack-assets-manifest": "^3.0.1",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.4",
    "webpack-manifest-plugin": "^2.0.3",
    "webpack-md5-hash": "^0.0.6",
    "webpack-merge": "^4.1.2",
    "webpack-spritesmith": "^0.5.2",
    "yorkie": "^1.0.3"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "iOS 8",
    "not ie <= 8"
  ],
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "$",
      "Jquery",
      "console",
      "alert",
      "window"
    ],
    "plugins": [
      "html"
    ],
    "ignore": [
      "**/dist/",
      "**/build/",
      "**/config/",
      "**/node_modules/"
    ]
  },
  "template": {
    "// 1": "input&output路径都是相对于package.json文件, 输入文件不能直接以.json结尾",
    "// 2": "单模板输入输出时，允许input，output可以是string，多模板输入输出时，必须为数组",
    "assetsRoot": "../public/dist",
    "input": [
      "_assets.php"
    ],
    "output": [
      "../config/_assets.php"
    ]
  }
}
