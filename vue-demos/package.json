{
  "name": "vue-cli3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:sentry": "vue-cli-service build --mode sentry",
    "watch": "vue-cli-service build --mode watch --watch",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue": "^2.5.17",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@sentry/browser": "^4.3.0",
    "@sentry/webpack-plugin": "^1.6.1",
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/eslint-config-standard": "^3.0.5",
    "axios": "^0.18.0",
    "imports-loader": "^0.8.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "vue-awesome-swiper": "^3.1.3",
    "vue-template-compiler": "^2.5.17",
    "webpack-assets-manifest": "^3.1.0"
  },
  "assets": {
    "root": {
      "watch": "../public/dist.watch",
      "build": "../public/dist"
    },
    "publicPath": {
      "watch": "./dist.watch/",
      "build": "./dist/"
    }
  },
  "nodeSassConfig": {
    "binarySite": "https://npm.taobao.org/mirrors/node-sass"
  }
}
